<script type="text/javascript">

    function SearchBranch()
    {
        $.getJSON('../NonStockedMaster/AdmTaxList', function (data) {
            var items = '<table id="TaxSearch" class="table table-hover table-striped table-condensed table-responsive table-bordered"><tr><th>Tax Code</th><th>Description</th></tr>';
            $.each(data, function (i, tax) {

                items += "<tr class='nr' data-dismiss='modal'><td>" + tax.TaxCode + "</td><td>" + tax.Description + "</td></tr>";


            });
            items += "</table>";
            $('#taxResults').html(items);
            //alert(items);
        });
    }




    $(document).ready(function () {

        SearchBranch();

        //$('#txtSearchBranch').keyup(function () {
        //    SearchBranch();
        //});

        $('#taxResults').on("click", ".nr", function () {
            var TaxCode = $(this).find('td:eq(0)').text();
           

            $('#txtTaxCode').val(TaxCode);
        });
    });
</script>